    <span class="kd">public</span> <span class="kd">static</span> <span class="kt">int</span> <span class="nf">Calculation</span><span class="o">(</span><span class="kt">int</span><span class="o">[]</span> <span class="n">operand</span><span class="o">,</span> <span class="nc">String</span> <span class="n">operator</span><span class="o">)</span> <span class="o">{</span>
