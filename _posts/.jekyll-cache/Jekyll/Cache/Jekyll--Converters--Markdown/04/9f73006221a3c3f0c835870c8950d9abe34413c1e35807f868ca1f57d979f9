                    <span class="k">break</span><span class="o">;</span>
                <span class="k">case</span> <span class="sc">'-'</span><span class="o">:</span>
                    <span class="n">result</span> <span class="o">-=</span> <span class="n">operand</span><span class="o">[</span><span class="n">i</span> <span class="o">+</span> <span class="mi">1</span><span class="o">];</span>
